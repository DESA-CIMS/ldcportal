<?xml version="1.0"?>

<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd">
	<persistence-unit-metadata>
		<xml-mapping-metadata-complete/>
		<persistence-unit-defaults>
			<access>PROPERTY</access>
		</persistence-unit-defaults>
	</persistence-unit-metadata>
	<mapped-superclass class="org.un.ldcportal.ismapplications.model.impl.ISMModelImpl">
		<attributes>
			<id name="ismId" />
			<basic name="webContentId" />
			<basic name="moderatorId" />
			<basic name="companyId" />
			<basic name="groupId" />
			<basic name="mbCategoryId" />
			<basic name="assetCategoryId" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
		</attributes>
	</mapped-superclass>
	<mapped-superclass class="org.un.ldcportal.ismapplications.model.impl.ISMApplicationModelImpl">
		<attributes>
			<id name="ismApplicationId" />
			<basic name="uuid">
				<column name="UUID_" />
			</basic>
			<basic name="country" />
			<basic name="ismId" />
			<basic name="status" />
			<basic name="statusByUserId" />
			<basic name="statusByUserName" />
			<basic name="statusDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="companyId" />
			<basic name="groupId" />
			<basic name="userId" />
			<basic name="applicationReason" />
			<basic name="applicationAdditional" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
			<transient name="statusByUserUuid" />
			<transient name="userUuid" />
		</attributes>
	</mapped-superclass>
	<entity class="org.un.ldcportal.ismapplications.model.impl.ISMImpl" name="ISM">
		<table name="ISM_ISM" />
		<attributes />
	</entity>
	<entity class="org.un.ldcportal.ismapplications.model.impl.ISMApplicationImpl" name="ISMApplication">
		<table name="ISM_ISMApplication" />
		<attributes />
	</entity>
</entity-mappings>