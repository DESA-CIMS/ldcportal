<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_0_0.dtd">
<service-builder package-path="org.un.ldcportal.jargon">
    <author>Thibault Boyeux</author>
    <namespace>LDCJargon</namespace>
    <entity name="LDCJargon" remote-service="false" local-service="true">
        <column name="ldcJargonId" type="long" primary="true"></column>
        <column name="name" type="String"></column>
        <column name="definition" type="String"></column>
        <column name="companyId" type="long"></column>
        <column name="groupId" type="long"></column>
        <finder name="GroupId" return-type="Collection">
            <finder-column name="groupId"></finder-column>
        </finder>
    </entity>
</service-builder>